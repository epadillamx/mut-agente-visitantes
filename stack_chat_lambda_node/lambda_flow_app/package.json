{
  "name": "whatsapp-flow-incidencias",
  "version": "1.0.0",
  "description": "Backend for WhatsApp Flow incident reporting system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node scripts/test-flow.js",
    "db:create": "node scripts/create-dynamodb-table.js create",
    "db:delete": "node scripts/create-dynamodb-table.js delete",
    "docker:build": "docker build -t whatsapp-flow-incidencias .",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down"
  },
  "keywords": ["whatsapp", "flow", "incidencias", "dynamodb"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "@aws-sdk/client-dynamodb": "^3.699.0",
    "@aws-sdk/lib-dynamodb": "^3.699.0",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.1",
    "@aws-sdk/client-secrets-manager": "^3.600.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
